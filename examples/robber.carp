(defn expand-char [c]
  (cond
    (= @c \a) (str c)
    (= @c \o) (str c)
    (= @c \u) (str c)
    (= @c \e) (str c)
    (= @c \i) (str c)
    (= @c \y) (str c)
    ;;true (str* c "o" c)
    ))

(defn robber [text]
  (string-join "" &(map-copy expand-char &(chars text))))

(defn robber-main []
  (while true
    (robber "erik")))
