(use Maybe)

;; Variable shadowing inside match
(defn confusion [x]
  (match x
    (Maybe.Just x) x
    (Maybe.Nothing) @"Nope"))

;; Recursive sumtype
(deftype JSON
  (Str [String])
  (Num [Double])
  (Arr [(Array JSON)])
  (Obj [(Map String JSON)]))

(defn main []
  (println* (confusion (Just @"Yo"))))
