(Project.config "print-ast" true)

;; Syntax D
(deftype (Maybe a)
  (Just [a])
  (Nothing []))

(deftype Flip (A [Int]) (B [Bool]))

(defn f []
  (Flip.A 123))

(defn g []
  (Flip.B true))

(defn m []
  (Maybe.Just 123))

(defn n []
  (the (Maybe Int) (Maybe.Nothing)))

(defn main []
  (match (Flip.A 100)
    (A a) 1
    (B b) 2))
