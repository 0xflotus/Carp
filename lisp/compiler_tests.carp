        solution (solve-constraints constraints)
        ;;_ (println "\n- Backwards -")
        solution-backwards (solve-constraints (reverse constraints))]
    (do
      (assert-eq solution {"t0" :bool "t1" :float})
      (assert-eq solution solution-backwards))))

